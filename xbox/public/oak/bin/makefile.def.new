MFC_LIBS=$(SDK_LIB_PATH)\nafxcw.lib
!     endif
!    endif

!   else               # USE_STATIC_MFC

!    ifdef DEBUG_CRTS
!     ifdef USE_MFCUNICODE
MFC_STATIC_LIB = $(SDK_LIB_PATH)\mfcs$(MFC_VER)ud.lib
